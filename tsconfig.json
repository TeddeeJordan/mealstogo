{
  "extends": "expo/tsconfig.base",
  "include": [
    "src/components",
    "src/custom.d.ts"
  ],
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "rootDir": ".",
    "paths": {
      "*": ["./src/*"],
      "@assets/*": ["./assets"],
      "@components/*": ["./src/components"],
      "@screens/*": ["./src/screens"],
      "@navigation/*": ["./src/navigation"],
      "@constants/*": ["./src/constants"],
    }
  }
}
